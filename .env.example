# yQuant.NET Environment Variables
# Copy this file to .env.local and fill in your actual values
# .env.local is gitignored and will not be committed

# ============================================
# Redis Configuration
# ============================================
Redis=localhost:6379

# ============================================
# Multi-Account Configuration
# ============================================
# Account 1
Accounts__0__Alias=MainAccount
Accounts__0__UserId=
Accounts__0__BrokerType=KIS
Accounts__0__AccountNumber=
Accounts__0__Credentials__AppKey=
Accounts__0__Credentials__AppSecret=
Accounts__0__Credentials__BaseUrl=https://openapi.koreainvestment.com:9443

# Account 2 (optional, add more as needed)
# Accounts__1__Alias=SubAccount
# Accounts__1__UserId=
# Accounts__1__BrokerType=KIS
# Accounts__1__AccountNumber=
# Accounts__1__Credentials__AppKey=
# Accounts__1__Credentials__AppSecret=
# Accounts__1__Credentials__BaseUrl=https://openapi.koreainvestment.com:9443

# ============================================
# Telegram Notifications
# ============================================
Telegram__BotToken=
Telegram__ChatId=

# ============================================
# Discord Notifications
# ============================================
Discord__Enabled=true
Discord__DefaultWebhookUrl=https://discord.com/api/webhooks/fallback
Discord__TimeoutMs=2000

# System channels (status, errors)
Discord__System__Status=https://discord.com/api/webhooks/system_status_id/token
Discord__System__Error=https://discord.com/api/webhooks/system_error_id/token

# Signal channels (strategy-based routing)
Discord__Signal__Mappings__Intraday=https://discord.com/api/webhooks/intraday_id/token
Discord__Signal__Mappings__General=https://discord.com/api/webhooks/general_signal_id/token
Discord__Signal__Mappings__TrendFollow_A=https://discord.com/api/webhooks/strategy_a_id/token

# Account-specific channels (per account alias)
# Discord__Accounts__MainAccount__Description=Main Trading Account
# Discord__Accounts__MainAccount__Channels__Execution=https://discord.com/api/webhooks/main_exec_id/token
# Discord__Accounts__MainAccount__Channels__Error=https://discord.com/api/webhooks/main_error_id/token
# Discord__Accounts__MainAccount__Channels__Summary=https://discord.com/api/webhooks/main_summary_id/token

# ============================================
# Webhook Security (for yQuant.App.Webhook)
# ============================================
Security__WebhookSecret=your_webhook_secret_here
# Allowed IPs for webhook requests (TradingView servers)
Security__AllowedIps__0=52.89.214.238
Security__AllowedIps__1=34.212.75.30
Security__AllowedIps__2=54.218.53.128
Security__AllowedIps__3=52.32.178.7
