[Unit]
Description=yQuant Frontend Dashboard Service
After=network.target

[Service]
Type=notify
WorkingDirectory=/srv/yquant/frontend
ExecStart=/usr/bin/dotnet /srv/yquant/frontend/yQuant.App.Web.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=frontend
Environment=ASPNETCORE_URLS=http://0.0.0.0:5000

[Install]
WantedBy=default.target
